{
  "name": "rain-misty",
  "version": "0.0.3",
  "private": false,
  "main": "dist/rm.umd.min.js",
  "description": "vue-ui",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name rm src/index.js",
    "build:modern": "vue-cli-service build --modern --dest docs/.vuepress/public/dist",
    "test:unit": "vue-cli-service test:unit",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "deploy": "gh-pages -d docs/.vuepress/dist",
    "deploy:all": "npm run build && gh-pages -d docs/.vuepress/dist",
    "lint": "eslint --ext .js,.vue src",
    "fix": "eslint --fix --ext .js,.vue src --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atJiangBei/ui.git"
  },
  "keywords": [
    "UI",
    "vue",
    "rain-misty"
  ],
  "author": "jiangbei",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/atJiangBei/ui/issues"
  },
  "homepage": "https://github.com/atJiangBei/ui#readme",
  "dependencies": {
    "gh-pages": "^2.0.1",
    "vue": "^2.6.10",
    "vue-highlightjs": "^1.3.3",
    "vuepress": "^1.0.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.6.0",
    "@vue/cli-plugin-unit-mocha": "^3.6.0",
    "@vue/cli-service": "^3.6.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "@vuepress/plugin-nprogress": "^1.0.0-rc.1",
    "chai": "^4.1.2",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    "small-swiper": "^1.0.7",
    "vue-router": "^3.1.3",
    "vue-template-compiler": "^2.5.21"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
